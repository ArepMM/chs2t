<?xml version="1.0" encoding="UTF-8"?>
<Config>
    <Device>
        <Order>2</Order>
        <SubStepNum>1</SubStepNum>
        <Solver>euler</Solver>

        <!-- Объём нижней камеры ДАКО (повторяющей давление от КВТ и клапана экстренного торможения на скорости выше U6) -->
        <V0>0.0005</V0>

        <!-- Объём средней камеры ДАКО (повторяющей давление от воздухораспределителя через отпускной клапан) -->
        <V1>0.0005</V1>

        <!-- Скорость, ниже которой отключается реостатное торможение, км/ч (измеряется на первой оси) -->
        <U1>40.0</U1>

        <!-- Скорость, выше которой повышается давление в ТЦ от КВТ и экстренного, км/ч (измеряется на шестой оси) -->
        <U6>80.0</U6>

        <!-- Коэффицент к потоку на наполнение ТЦ -->
        <K1>1.0e-2</K1>
        <!-- Коэффицент к потоку на опорожнение ТЦ -->
        <K2>1.5e-2</K2>

        <!-- Коэффицент к потоку на наполнение нижней камеры от КВТ и экстренного -->
        <K3>3e-3</K3>
        <!-- Коэффицент к потоку на опорожнение нижней камеры в атмосферу -->
        <K4>4e-3</K4>

        <!-- Коэффицент к потоку на наполнение и опорожнение средней камеры вслед за давлением от воздухораспределителя -->
        <K5>4e-3</K5>

        <!-- Коэффицент к давлению в ТЦ от нижней камеры -->
        <A1>0.71</A1>

        <!-- Коэффицент к давлению в ТЦ от средней камеры -->
        <A2>1.0</A2>

        <!-- Коэффиценты к условному положению центрального штока -->
        <k1>2</k1>
        <k2>2</k2>

        <!-- Коэффиценты к условному положению поршня в центробежном регуляторе на шестой оси -->
        <k3>10</k3>
        <k4>10</k4>
    </Device>
</Config>
